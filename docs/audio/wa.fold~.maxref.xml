<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.fold~" patcher_type="standard">
    <digest>a wave folder</digest>
    <description>a simple wavefolder</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="signal">
            <digest/>
            <description>the folded signal</description>
        </entry>
    </misc>
    <objarglist/>
    <attributelist>
        <attribute name="boost" set="1" type="float">
            <digest>boost (2.5)</digest>
            <description>boost the incoming signal</description>
        </attribute>
        <attribute name="offset" set="1" type="symbol">
            <digest>offset (0.0)</digest>
            <description>offset the incoming signal</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="signal" standard="1">
            <arglist>
                <arg name="signal" optional="0" type="signal"/>
            </arglist>
            <digest/>
            <description>in left inlet: the signal to fold</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
