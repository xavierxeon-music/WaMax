<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.pingpong~" patcher_type="standard">
    <digest>ping pong delay</digest>
    <description>a transport synced ping poly delay</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>right signal out</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>delay 1 time</digest>
        </objarg>
        <objarg name="2" optional="1" type="symbol">
            <digest>delay 1 time</digest>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="feedback" set="1" type="symbol">
            <digest>feedback</digest>
            <description>feddback amount</description>
        </attribute>
        <attribute name="filter" set="1" type="symbol">
            <digest>filter cutoff</digest>
            <description>cutoff freqeuncy of the lowpass filter in the feddback line</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="feedback" standard="0">
            <arglist>
                <arg name="feedback" optional="0" type="symbol"/>
            </arglist>
            <digest>feedback</digest>
            <description>feddback amount</description>
        </method>
        <method name="filter" standard="0">
            <arglist>
                <arg name="filter" optional="0" type="symbol"/>
            </arglist>
            <digest>filter cutoff</digest>
            <description>cutoff freqeuncy of the lowpass filter in the feddback line</description>
        </method>
        <method name="signal" standard="1">
            <arglist>
                <arg name="signal" optional="0" type="signal"/>
            </arglist>
            <digest/>
            <description>the input signal</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
