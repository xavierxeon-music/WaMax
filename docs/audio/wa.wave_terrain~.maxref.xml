<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.wave_terrain~" patcher_type="standard">
    <digest>Wave Terrain Synthesis</digest>
    <description>Scan a matrix and report its plane values</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
        </entry>
    </misc>
    <objarglist>
        <objarg name="" optional="0" type="symbol">
            <digest> (input)</digest>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="offColor" set="1" type="symbol">
            <digest/>
        </attribute>
        <attribute name="offDistance" set="1" type="int">
            <digest/>
        </attribute>
        <attribute name="px" set="1" type="symbol">
            <digest/>
        </attribute>
        <attribute name="py" set="1" type="int">
            <digest/>
        </attribute>
        <attribute name="radius" set="1" type="int">
            <digest/>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="offColor" standard="0">
            <arglist>
                <arg name="offColor" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="offDistance" standard="0">
            <arglist>
                <arg name="offDistance" optional="0" type="int"/>
            </arglist>
            <digest/>
        </method>
        <method name="py" standard="0">
            <arglist>
                <arg name="py" optional="0" type="int"/>
            </arglist>
            <digest/>
        </method>
        <method name="radius" standard="0">
            <arglist>
                <arg name="radius" optional="0" type="int"/>
            </arglist>
            <digest/>
        </method>
        <method name="rx" standard="0">
            <arglist>
                <arg name="rx" optional="0" type="anything"/>
            </arglist>
            <digest/>
        </method>
        <method name="ry" standard="0">
            <arglist>
                <arg name="ry" optional="0" type="anything"/>
            </arglist>
            <digest/>
        </method>
        <method name="signal" standard="1">
            <arglist>
                <arg name="signal" optional="0" type="signal"/>
            </arglist>
            <digest>x/y control</digest>
            <description>(left) x control<br/>(right) y control</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
