<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.patch.storage" patcher_type="standard">
    <digest>patch storage</digest>
    <description>load and save <o>pattr</o></description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>(left) attach to <o>pattrstorage</o><br/><br/>(right) the storeage file name</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="load" optional="1" type="symbol">
            <digest>load storage</digest>
            <description>load from storage</description>
        </objarg>
        <objarg name="clear" optional="1" type="symbol">
            <digest>clear storage</digest>
            <description>clear the storage</description>
        </objarg>
        <objarg name="save" optional="1" type="symbol">
            <digest>save storage</digest>
            <description>save to storage</description>
        </objarg>
    </objarglist>
    <attributelist/>
    <methodlist>
        <method name="clear" standard="0">
            <arglist>
                <arg name="clear" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="load" standard="0">
            <arglist>
                <arg name="load" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
        <method name="save" standard="0">
            <arglist>
                <arg name="save" optional="0" type="symbol"/>
            </arglist>
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
