<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.push2.pad" patcher_type="standard">
    <digest>a pad on a push device</digest>
    <description>a pad on a push device</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>bang when pad is released.</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="name" optional="1" type="symbol">
            <digest>the name of the pad</digest>
            <description>the name of the pad. see <o>wa.push.device~</o> for details</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="color" set="1" type="symbol">
            <digest>color of the pad</digest>
            <description>rgb hex color of the pad, default is  ffffff</description>
        </attribute>
        <attribute name="downColor" set="1" type="symbol">
            <digest>color of the pressed pad</digest>
            <description>rgb hex color of the pad, default is  blue</description>
        </attribute>
        <attribute name="toggle" set="1" type="symbol">
            <digest>make this a toggle pad</digest>
            <description>make this a toggle pad, default is 0</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="color" standard="0">
            <arglist>
                <arg name="color" optional="0" type="symbol"/>
            </arglist>
            <digest>color of the pad</digest>
            <description>rgb hex color of the pad, default is  ffffff</description>
        </method>
        <method name="downColor" standard="0">
            <arglist>
                <arg name="downColor" optional="0" type="symbol"/>
            </arglist>
            <digest>color of the pressed pad</digest>
            <description>rgb hex color of the pad, default is  blue</description>
        </method>
        <method name="forceActive" standard="0">
            <arglist>
                <arg name="forceActive" optional="0" type="symbol"/>
            </arglist>
            <digest>force the active state</digest>
            <description>force the active state</description>
        </method>
        <method name="setPad" standard="0">
            <arglist>
                <arg name="setPad" optional="0" type="symbol"/>
            </arglist>
            <digest>set the pad id</digest>
            <description>set the pad id</description>
        </method>
        <method name="toggle" standard="0">
            <arglist>
                <arg name="toggle" optional="0" type="symbol"/>
            </arglist>
            <digest>make this a toggle pad</digest>
            <description>make this a toggle pad, default is 0</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
