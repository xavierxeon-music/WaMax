<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.midi.control" patcher_type="standard">
    <digest>midi control input</digest>
    <description>capture midi control values</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>bang if  value not equal to zero</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>device name</digest>
            <description>device name (default: "Internal LaunchKey")</description>
        </objarg>
        <objarg name="" optional="0" type="anything">
            <digest> (LaunchKey")</digest>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="number" set="1" type="int">
            <digest>controller number</digest>
            <description>the controller number to capture</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="number" standard="0">
            <arglist>
                <arg name="number" optional="0" type="int"/>
            </arglist>
            <digest>controller number</digest>
            <description>the controller number to capture</description>
        </method>
        <method name="int" standard="1">
            <arglist>
                <arg name="int" optional="0" type="int"/>
            </arglist>
            <digest/>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
