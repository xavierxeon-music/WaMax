<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="wa.midi.note" patcher_type="standard">
    <digest>midi note input</digest>
    <description>capture the velocity of a note</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>bang if  note off</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>device name</digest>
            <description>device name (default: "Internal LaunchKey")</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="value" set="1" type="int">
            <digest>midi note </digest>
            <description>which midi note to capture</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="value" standard="0">
            <arglist>
                <arg name="value" optional="0" type="int"/>
            </arglist>
            <digest>midi note </digest>
            <description>which midi note to capture</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
