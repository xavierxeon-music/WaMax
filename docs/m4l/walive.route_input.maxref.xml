<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>
<c74object name="walive.route_input" patcher_type="standard">
    <digest>live input routing</digest>
    <description>route the audio of a live channel to a plugin</description>
    <metadatalist>
        <metadata name="author">Ralf Waspe</metadata>
        <metadata name="tag">WaMax</metadata>
        <metadata name="tag">WaMaxPackage</metadata>
    </metadatalist>
    <misc name="Outputs">
        <entry name="anything">
            <digest/>
            <description>list of availabel tracks</description>
        </entry>
    </misc>
    <objarglist>
        <objarg name="1" optional="1" type="symbol">
            <digest>plugin index</digest>
            <description>the index of the plugin port (always stereo!)</description>
        </objarg>
    </objarglist>
    <attributelist>
        <attribute name="from" set="1" type="symbol">
            <digest>audio name</digest>
            <description>name of audio source</description>
        </attribute>
    </attributelist>
    <methodlist>
        <method name="from" standard="0">
            <arglist>
                <arg name="from" optional="0" type="symbol"/>
            </arglist>
            <digest>audio name</digest>
            <description>name of audio source</description>
        </method>
    </methodlist>
    <seealsolist/>
</c74object>
